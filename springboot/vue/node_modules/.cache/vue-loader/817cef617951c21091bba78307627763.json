l-button>\n    <el-popconfirm\n        class=\"ml-5\"\n        confirm-button-text='确定'\n        cancel-button-text='取消'\n        icon=\"el-icon-info\"\n        icon-color=\"red\"\n        title=\"确定批量删除勾选的数据吗？\"\n        @confirm=\"delBatch\"\n    >\n      <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n    </el-popconfirm>\n\n  </div>\n\n  <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\" @selection-change=\"handleSelectionChange\">\n    <el-table-column\n        type=\"selection\"\n        width=\"55\">\n    </el-table-column>\n    <el-table-column prop=\"id\" label=\"主键\" fit width=\"fit\">          </el-table-column>\n    <el-table-column prop=\"labno\" label=\"实验室编号\" width=\"100\">          </el-table-column>\n    <el-table-column prop=\"dutyUsrNo\" label=\"值班人编号\" >              </el-table-column>\n    <el-table-column prop=\"checkDate\" label=\"检查日期\" >         </el-table-column>\n    <el-table-column prop=\"checkDesc\" label=\"检查结果\" >         </el-table-column>\n    <el-table-column prop=\"remark\" label=\"备注\">         </el-table-column>\n    <el-table-column label=\"操作\"  width=\"200\" align=\"center\">\n      <template slot-scope=\"scope\">\n        <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\n        <el-popconfirm\n            class=\"ml-5\"\n            confirm-button-text='确定'\n            cancel-button-text='取消'\n            icon=\"el-icon-info\"\n            icon-color=\"red\"\n            title=\"确定删除吗？\"\n            @confirm=\"del(scope.row.id)\"\n        >\n          <el-button type=\"danger\" slot=\"reference\" >删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n        </el-popconfirm>\n      </template>\n    </el-table-column>\n  </el-table>\n  <div style=\"padding: 10px 0\">\n    <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"pageNum\"\n        :page-sizes=\"[5, 10, 15, 20]\"\n        :page-size=\"5\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\">\n    </el-pagination>\n  </div>\n\n  <el-dialog title=\"安全检查信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\n    <el-form label-width=\"80px\" size=\"big\">\n      <el-form-item label=\"实验室编号\">\n        <el-input v-model=\"form.labno\" autocomplete=\"off\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"值班人员编号\">\n        <el-input v-model.trim = \"form.dutyUsrNo\" autocomplete=\"off\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"检查结果\">\n        <el-select v-model=\"form.checkDesc\" placeholder=\"选择\" style=\"width: 100px\">\n          <el-option\n              v-for=\"item in options\"\n              :key=\"item.value\"\n              :label=\"item.label\"\n              :value=\"item.value\">\n          </el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-form-item label=\"备注信息\">\n        <el-input v-model=\"form.remark\" autocomplete=\"off\"></el-input>\n      </el-form-item>\n\n    </el-form>\n    <div slot=\"footer\" class=\"dialog-footer\">\n      <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n      <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n    </div>\n  </el-dialog>\n</div>\n</template>\n\n<script>\nexport default {\n  name: \"Secutitycheck\",\n  data(){\n    return{\n      options: [{\n        value: '合格',\n        label: '合格'\n      }, {\n        value: '不合格',\n        label: '不合格'\n      } ],\n      value: '',\n      tableData: [],\n      total:0,\n      pageNum:1,\n      pageSize:5,\n      labno:\"\",\n      dutyUsrNo:\"\",\n      //添加信息表单\n      dialogFormVisible:false,\n      form:{},\n      multipleSelection:[]\n    }\n  },\n  created(){\n    this.load()\n  },\n  methods:{\n\n    load(){\n      this.request.get(\"/securitycheck/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          labno: this.labno,\n          dutyusrno: this.dutyUsrNo\n        }\n      }).then(res => {\n            console.log(res)\n            this.tableData = res.records\n            this.total= res.total\n          })\n    },\n    save(){\n      this.request.post(\"/securitycheck\",this.form).then(res=>{\n        if(res) {\n          this.$message.success(\"添加成功\")\n          this.dialogFormVisible=false\n          this.load()\n        } else{\n          this.$message.error(\"添加失败\")\n        }\n      })\n    },\n    //*添加数据\n    handleAdd(){\n      this.dialogFormVisible=true\n      this.form={}\n    },\n    //*编辑数据\n    handleEdit(row){\n      this.form=row\n      this.dialogFormVisible=true\n    },\n    //*删除数据\n    del(id){\n      this.request.delete(\"/securitycheck/\"+id).then(res=>{\n        if(res){\n          this.$message.success(\"删除成功\")\n          this.load()\n        } else{\n          this.$message.error(\"删除失败\")\n        }\n      })\n    },\n    handleSelectionChange(val){\n      console.log(val)\n      this.multipleSelection=val\n    },\n    delBatch(){\n      let ids=this.multipleSelection.map(v => v.id)//把对象数组编程纯数字数组[{} {} {}]->[1,2,3]\n      this.request.post(\"/securitycheck/del/batch\",ids).then(res=>{\n        if(res){\n          this.$message.success(\"批量删除成功\")\n          this.load()\n        } else{\n          this.$message.error(\"批量删除失败\")\n        }\n      })\n    },\n    reset(){\n      this.username=\"\"\n      this.level=\"\"\n      this.name=\"\"\n      this.load()\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize)\n      this.pageSize=pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum)\n      this.pageNum=pageNum\n      this.load()\n    },\n    exp(){\n      window.open(\"http://localhost:9090/user/export\")\n    },\n    handleExcelImportSuccess(){\n      this.$message.success(\"导入成功\")\n      this.load()\n    }\n  }\n}\n</script>\n\n<style scoped>\n.headerBg {\n  background: #eee!important;\n}\n</style>"]}]}